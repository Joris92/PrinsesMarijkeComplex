    edge South_Upstream_EnteringTLs_West_Actuator_c_r when South_HW_EnteringTL_BVH_West_Q_Red = true and (South_HW_EnteringTL_BVH_West_Q_Green = false and (South_HW_EnteringTL_BVH_West_Q_RedRed = false and (South_Upstream_EnteringTLs_West_Actuator = RedRed or (South_Upstream_EnteringTLs_West_Actuator = RedGreen or South_Upstream_EnteringTLs_West_Actuator = Green)))) do if South_Upstream_EnteringTLs_West_Actuator = RedRed: South_Upstream_EnteringTLs_West_Actuator := Red elif South_Upstream_EnteringTLs_West_Actuator = RedGreen: South_Upstream_EnteringTLs_West_Actuator := Red elif South_Upstream_EnteringTLs_West_Actuator = Green: South_Upstream_EnteringTLs_West_Actuator := Red end;
