    edge South_Upstream_EnteringTLs_West_SensorR2_u_off when South_Upstream_EnteringTLs_West_Actuator != RedRed and South_Upstream_EnteringTLs_West_SensorR2 = On do South_HW_EnteringTL_BVH_West_RedRed := false, South_Upstream_EnteringTLs_West_SensorR2 := Off;
    edge South_Upstream_EnteringTLs_West_SensorR2_u_on when South_Upstream_EnteringTLs_West_Actuator = RedRed and South_Upstream_EnteringTLs_West_SensorR2 = Off do South_HW_EnteringTL_BVH_West_RedRed := true, South_Upstream_EnteringTLs_West_SensorR2 := On;
    edge South_Upstream_EqualWaterSensor_u_off when South_Upstream_EqualWaterSensor = Equal and South_Water_H_Chamber < if Wh = High: 5.8 else 4.6 end do South_HW_EqualWaterSensor_BVH_Equal := false, South_Upstream_EqualWaterSensor := Unequal;
    edge South_Upstream_EqualWaterSensor_u_on when South_Upstream_EqualWaterSensor = Unequal and South_Water_H_Chamber >= if Wh = High: 5.8 else 4.6 end do South_HW_EqualWaterSensor_BVH_Equal := true, South_Upstream_EqualWaterSensor := Equal;
    edge South_Upstream_Gates_East_Actuator_c_close when South_HW_Gate_BVH_East_Q_Close = true and South_Upstream_Gates_East_Actuator = Rest do South_Upstream_Gates_East_Actuator := Closing;
    edge South_Upstream_Gates_East_Actuator_c_emergencyStop when South_HW_Gate_BVH_East_Q_Close = false and (South_HW_Gate_BVH_East_Q_Open = false and (South_Upstream_Gates_East_Actuator = Opening or South_Upstream_Gates_East_Actuator = Closing)) do if South_Upstream_Gates_East_Actuator = Opening: South_Upstream_Gates_East_Actuator := Rest elif South_Upstream_Gates_East_Actuator = Closing: South_Upstream_Gates_East_Actuator := Rest end;
    edge South_Upstream_Gates_East_Actuator_c_endStopClosing when South_HW_Gate_BVH_East_Q_Close = false and South_Upstream_Gates_East_Actuator = Closing do South_Upstream_Gates_East_Actuator := Rest;
    edge South_Upstream_Gates_East_Actuator_c_endStopOpening when South_HW_Gate_BVH_East_Q_Open = false and South_Upstream_Gates_East_Actuator = Opening do South_Upstream_Gates_East_Actuator := Rest;
    edge South_Upstream_Gates_East_Actuator_c_open when South_HW_Gate_BVH_East_Q_Open = true and South_Upstream_Gates_East_Actuator = Rest do South_Upstream_Gates_East_Actuator := Opening;
    edge South_Upstream_Gates_East_Sensor_u_closed_off when South_Upstream_Gates_East_Degree < 90.0 and South_Upstream_Gates_East_Sensor = Closed do South_HW_Gate_BVH_East_Closed := false, South_Upstream_Gates_East_Sensor := Intermediate;
    edge South_Upstream_Gates_East_Sensor_u_closed_on when South_Upstream_Gates_East_Degree <= 0.0 and South_Upstream_Gates_East_Sensor = Intermediate do South_HW_Gate_BVH_East_Closed := true, South_Upstream_Gates_East_Sensor := Closed;
    edge South_Upstream_Gates_East_Sensor_u_open_off when South_Upstream_Gates_East_Degree > 0.0 and South_Upstream_Gates_East_Sensor = Open do South_HW_Gate_BVH_East_Open := false, South_Upstream_Gates_East_Sensor := Intermediate;
    edge South_Upstream_Gates_East_Sensor_u_open_on when South_Upstream_Gates_East_Degree >= 90.0 and South_Upstream_Gates_East_Sensor = Intermediate do South_HW_Gate_BVH_East_Open := true, South_Upstream_Gates_East_Sensor := Open;
    edge South_Upstream_Gates_West_Actuator_c_close when South_HW_Gate_BVH_West_Q_Close = true and South_Upstream_Gates_West_Actuator = Rest do South_Upstream_Gates_West_Actuator := Closing;
    edge South_Upstream_Gates_West_Actuator_c_emergencyStop when South_HW_Gate_BVH_West_Q_Close = false and (South_HW_Gate_BVH_West_Q_Open = false and (South_Upstream_Gates_West_Actuator = Opening or South_Upstream_Gates_West_Actuator = Closing)) do if South_Upstream_Gates_West_Actuator = Opening: South_Upstream_Gates_West_Actuator := Rest elif South_Upstream_Gates_West_Actuator = Closing: South_Upstream_Gates_West_Actuator := Rest end;
