    edge South_Upstream_Gates_West_Sensor_u_closed_off when South_Upstream_Gates_West_Degree < 90.0 and South_Upstream_Gates_West_Sensor = Closed do South_HW_Gate_BVH_West_Closed := false, South_Upstream_Gates_West_Sensor := Intermediate;
    edge South_Upstream_Gates_West_Sensor_u_closed_on when South_Upstream_Gates_West_Degree <= 0.0 and South_Upstream_Gates_West_Sensor = Intermediate do South_HW_Gate_BVH_West_Closed := true, South_Upstream_Gates_West_Sensor := Closed;
    edge South_Upstream_Gates_West_Sensor_u_open_off when South_Upstream_Gates_West_Degree > 0.0 and South_Upstream_Gates_West_Sensor = Open do South_HW_Gate_BVH_West_Open := false, South_Upstream_Gates_West_Sensor := Intermediate;
    edge South_Upstream_Gates_West_Sensor_u_open_on when South_Upstream_Gates_West_Degree >= 90.0 and South_Upstream_Gates_West_Sensor = Intermediate do South_HW_Gate_BVH_West_Open := true, South_Upstream_Gates_West_Sensor := Open;
    edge South_Upstream_LeavingTLs_East_Actuator_c_g when South_HW_LeavingTL_BVH_East_Q_Red = false and (South_HW_LeavingTL_BVH_East_Q_Green = true and South_Upstream_LeavingTLs_East_Actuator = Red) do South_Upstream_LeavingTLs_East_Actuator := Green;
    edge South_Upstream_LeavingTLs_East_Actuator_c_r when South_HW_LeavingTL_BVH_East_Q_Red = true and (South_HW_LeavingTL_BVH_East_Q_Green = false and South_Upstream_LeavingTLs_East_Actuator = Green) do South_Upstream_LeavingTLs_East_Actuator := Red;
    edge South_Upstream_LeavingTLs_East_SensorG_u_off when South_Upstream_LeavingTLs_East_Actuator = Red and South_Upstream_LeavingTLs_East_SensorG = On do South_HW_LeavingTL_BVH_East_Green := false, South_Upstream_LeavingTLs_East_SensorG := Off;
    edge South_Upstream_LeavingTLs_East_SensorG_u_on when South_Upstream_LeavingTLs_East_Actuator != Red and South_Upstream_LeavingTLs_East_SensorG = Off do South_HW_LeavingTL_BVH_East_Green := true, South_Upstream_LeavingTLs_East_SensorG := On;
    edge South_Upstream_LeavingTLs_East_SensorR_u_off when South_Upstream_LeavingTLs_East_Actuator = Green and South_Upstream_LeavingTLs_East_SensorR = On do South_HW_LeavingTL_BVH_East_Red := false, South_Upstream_LeavingTLs_East_SensorR := Off;
    edge South_Upstream_LeavingTLs_East_SensorR_u_on when South_Upstream_LeavingTLs_East_Actuator != Green and South_Upstream_LeavingTLs_East_SensorR = Off do South_HW_LeavingTL_BVH_East_Red := true, South_Upstream_LeavingTLs_East_SensorR := On;
    edge South_Upstream_LeavingTLs_West_Actuator_c_g when South_HW_LeavingTL_BVH_West_Q_Red = false and (South_HW_LeavingTL_BVH_West_Q_Green = true and South_Upstream_LeavingTLs_West_Actuator = Red) do South_Upstream_LeavingTLs_West_Actuator := Green;
    edge South_Upstream_LeavingTLs_West_Actuator_c_r when South_HW_LeavingTL_BVH_West_Q_Red = true and (South_HW_LeavingTL_BVH_West_Q_Green = false and South_Upstream_LeavingTLs_West_Actuator = Green) do South_Upstream_LeavingTLs_West_Actuator := Red;
    edge South_Upstream_LeavingTLs_West_SensorG_u_off when South_Upstream_LeavingTLs_West_Actuator = Red and South_Upstream_LeavingTLs_West_SensorG = On do South_HW_LeavingTL_BVH_West_Green := false, South_Upstream_LeavingTLs_West_SensorG := Off;
    edge South_Upstream_LeavingTLs_West_SensorG_u_on when South_Upstream_LeavingTLs_West_Actuator != Red and South_Upstream_LeavingTLs_West_SensorG = Off do South_HW_LeavingTL_BVH_West_Green := true, South_Upstream_LeavingTLs_West_SensorG := On;
    edge South_Upstream_LeavingTLs_West_SensorR_u_off when South_Upstream_LeavingTLs_West_Actuator = Green and South_Upstream_LeavingTLs_West_SensorR = On do South_HW_LeavingTL_BVH_West_Red := false, South_Upstream_LeavingTLs_West_SensorR := Off;
    edge South_Upstream_LeavingTLs_West_SensorR_u_on when South_Upstream_LeavingTLs_West_Actuator != Green and South_Upstream_LeavingTLs_West_SensorR = Off do South_HW_LeavingTL_BVH_West_Red := true, South_Upstream_LeavingTLs_West_SensorR := On;
    edge South_Upstream_Paddles_East_Actuator_c_close when South_HW_Paddle_BVH_East_Q_Close = true and South_Upstream_Paddles_East_Actuator = Rest do South_Upstream_Paddles_East_Actuator := Closing;
    edge South_Upstream_Paddles_East_Actuator_c_emergencyStop when South_HW_Paddle_BVH_East_Q_Close = false and (South_HW_Paddle_BVH_East_Q_Open = false and (South_Upstream_Paddles_East_Actuator = Opening or South_Upstream_Paddles_East_Actuator = Closing)) do if South_Upstream_Paddles_East_Actuator = Opening: South_Upstream_Paddles_East_Actuator := Rest elif South_Upstream_Paddles_East_Actuator = Closing: South_Upstream_Paddles_East_Actuator := Rest end;
    edge South_Upstream_Paddles_East_Actuator_c_endStopClosing when South_HW_Paddle_BVH_East_Q_Close = false and South_Upstream_Paddles_East_Actuator = Closing do South_Upstream_Paddles_East_Actuator := Rest;
    edge South_Upstream_Paddles_East_Actuator_c_endStopOpening when South_HW_Paddle_BVH_East_Q_Open = false and South_Upstream_Paddles_East_Actuator = Opening do South_Upstream_Paddles_East_Actuator := Rest;
    edge South_Upstream_Paddles_East_Actuator_c_open when South_HW_Paddle_BVH_East_Q_Open = true and South_Upstream_Paddles_East_Actuator = Rest do South_Upstream_Paddles_East_Actuator := Opening;
    edge South_Upstream_Paddles_East_Sensor_u_closed_off when South_Upstream_Paddles_East_Degree < 90.0 and South_Upstream_Paddles_East_Sensor = Closed do South_HW_Paddle_BVH_East_Closed := false, South_Upstream_Paddles_East_Sensor := Intermediate;
    edge South_Upstream_Paddles_East_Sensor_u_closed_on when South_Upstream_Paddles_East_Degree <= 0.0 and South_Upstream_Paddles_East_Sensor = Intermediate do South_HW_Paddle_BVH_East_Closed := true, South_Upstream_Paddles_East_Sensor := Closed;
    edge South_Upstream_Paddles_East_Sensor_u_open_off when South_Upstream_Paddles_East_Degree > 0.0 and South_Upstream_Paddles_East_Sensor = Open do South_HW_Paddle_BVH_East_Open := false, South_Upstream_Paddles_East_Sensor := Intermediate;
    edge South_Upstream_Paddles_East_Sensor_u_open_on when South_Upstream_Paddles_East_Degree >= 90.0 and South_Upstream_Paddles_East_Sensor = Intermediate do South_HW_Paddle_BVH_East_Open := true, South_Upstream_Paddles_East_Sensor := Open;
    edge South_Upstream_Paddles_West_Actuator_c_close when South_HW_Paddle_BVH_West_Q_Close = true and South_Upstream_Paddles_West_Actuator = Rest do South_Upstream_Paddles_West_Actuator := Closing;
    edge South_Upstream_Paddles_West_Actuator_c_emergencyStop when South_HW_Paddle_BVH_West_Q_Close = false and (South_HW_Paddle_BVH_West_Q_Open = false and (South_Upstream_Paddles_West_Actuator = Opening or South_Upstream_Paddles_West_Actuator = Closing)) do if South_Upstream_Paddles_West_Actuator = Opening: South_Upstream_Paddles_West_Actuator := Rest elif South_Upstream_Paddles_West_Actuator = Closing: South_Upstream_Paddles_West_Actuator := Rest end;
    edge South_Upstream_Paddles_West_Actuator_c_endStopClosing when South_HW_Paddle_BVH_West_Q_Close = false and South_Upstream_Paddles_West_Actuator = Closing do South_Upstream_Paddles_West_Actuator := Rest;
    edge South_Upstream_Paddles_West_Actuator_c_endStopOpening when South_HW_Paddle_BVH_West_Q_Open = false and South_Upstream_Paddles_West_Actuator = Opening do South_Upstream_Paddles_West_Actuator := Rest;
    edge South_Upstream_Paddles_West_Actuator_c_open when South_HW_Paddle_BVH_West_Q_Open = true and South_Upstream_Paddles_West_Actuator = Rest do South_Upstream_Paddles_West_Actuator := Opening;
    edge South_Upstream_Paddles_West_Sensor_u_closed_off when South_Upstream_Paddles_West_Degree < 90.0 and South_Upstream_Paddles_West_Sensor = Closed do South_HW_Paddle_BVH_West_Closed := false, South_Upstream_Paddles_West_Sensor := Intermediate;
    edge South_Upstream_Paddles_West_Sensor_u_closed_on when South_Upstream_Paddles_West_Degree <= 0.0 and South_Upstream_Paddles_West_Sensor = Intermediate do South_HW_Paddle_BVH_West_Closed := true, South_Upstream_Paddles_West_Sensor := Closed;
    edge South_Upstream_Paddles_West_Sensor_u_open_off when South_Upstream_Paddles_West_Degree > 0.0 and South_Upstream_Paddles_West_Sensor = Open do South_HW_Paddle_BVH_West_Open := false, South_Upstream_Paddles_West_Sensor := Intermediate;
    edge South_Upstream_Paddles_West_Sensor_u_open_on when South_Upstream_Paddles_West_Degree >= 90.0 and South_Upstream_Paddles_West_Sensor = Intermediate do South_HW_Paddle_BVH_West_Open := true, South_Upstream_Paddles_West_Sensor := Open;
    edge Wh_high when HW_Wh_HighWater do if Wh = High: Wh := High elif Wh = Low: Wh := High end;
    edge Wh_low when not HW_Wh_HighWater do if Wh = High: Wh := Low elif Wh = Low: Wh := Low end;
end
